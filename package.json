{
  "name": "gaivota-test-api",
  "version": "1.0.0",
  "description": "Gaivota Fullstack test API",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src/**/*.{js,spec.js} --no-error-on-unmatched-pattern",
    "lint:check": "npm run lint",
    "lint:write": "npm run lint -- --fix",
    "prettier": "prettier --config .prettierrc  \"src/**/*.{js,spec.js}\"",
    "prettier:check": "npm run prettier -- --check",
    "prettier:write": "npm run prettier -- --write",
    "linter": "npm run lint:check && npm run prettier:check",
    "test": "echo \"Error: no test specified\" && exit 1",
    "quality": "npm run linter && npm run test",
    "clean": "rimraf dist",
    "transpile": "babel ./src --out-dir dist",
    "build": "npm run clean && npm run transpile",
    "start": "node ./dist/server.js",
    "dev": "NODE_ENV=development npm run build && npm run start",
    "dev:watch": "nodemon",
    "precommit": "npm run linter",
    "prepush": "npm run precommit"
  },
  "author": "Wallace Gleen <wallace.gleen@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.13.10",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.11.19",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "neat-csv": "^6.0.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/eslint-plugin": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-regenerator": "^7.12.13",
    "@babel/preset-env": "^7.13.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2"
  }
}
